<div class="modal fade bd-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel"
  aria-hidden="true">
  <div class="modal-dialog modal-lg">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLongTitle">Enregistrement des UES</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <form action="" method="post">
            <div class="row">

                <div class="col-6">
                    <div class="form-group">
                        <div class="col-4">Référence : </div>
                        <div class="col-10">
                            <input type="text" name="reference" id="reference" class="form-control">
                           
                                <span class="text-danger"></span>
                            
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-4">Semestre : </div>
                        <div class="col-10">
                            <input type="number" name="semestre" id="semestre" class="form-control">
                            
                                <span class="text-danger"></span>
                            
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-6">Titre :</div>
                        <div class="col-10">
                            <input type="text" name="titre" id="titre" class="form-control">
                            
                                <span class="text-danger"></span>
                            
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-6">ECTS :</div>
                        <div class="col-10">
                            <input type="number" name="ects" id="ects" class="form-control">
                            
                                <span class="text-danger"></span>
                            
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-6">Type :</div>
                        <div class="col-10">
                            <input type="text" name="type" id="type" class="form-control">
                            
                                <span class="text-danger"></span>
                            
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-6">Statut :</div>
                        <div class="col-10">
                            <select name="statut" id="statut" class="form-control">
                                <option value=""></option>
                                <option value="obligatoire">Obligatoire</option>
                                <option value="optionnel">Optionnel</option>
                            </select>
                                <span class="text-danger"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-6">Connaissances :</div>
                        <div class="col-10">
                            <select name="conn" id="conn" class="form-control">
                                <option value=""></option>
                                <option value="conn_1">CONN_1</option>
                                <option value="conn_2">CONN_2</option>
                                <option value="conn_3">CONN_3</option>
                            </select>
                                <span class="text-danger"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-6">Compétences :</div>
                        <div class="col-10">
                            <select name="comp" id="comp" class="form-control">
                                <option value=""></option>
                                <option value="comp_1">COMP_1</option>
                                <option value="comp_2">COMP_2</option>
                                <option value="comp_3">COMP_3</option>
                            </select>
                                <span class="text-danger"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-6">Prérequis :</div>
                        <div class="col-10">
                            <input type="text" name="prerequis" id="prerequis" class="form-control">
                            
                                <span class="text-danger"></span>
                            
                        </div>
                    </div>
                </div>
                <div class="col-6">
                    <div class="form-group">
                        <div class="col-6">CM :</div>
                        <div class="col-10">
                            <input type="number" name="cm" id="cm" class="form-control">
                            
                                <span class="text-danger"></span>
                            
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-4">TD :</div>
                        <div class="col-10">
                            <input type="number" name="td" id="td" class="form-control">
                            
                                <span class="text-danger"></span>
                            
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-8">TP :</div>
                        <div class="col-10">
                            <input type="number" name="tp" id="tp" class="form-control">
                            
                                <span class="text-danger"></span>
                            
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-4">Effectif :</div>
                        <div class="col-10">
                            <input type="number" name="effectif" id="effectif" class="form-control">
                            
                                <span class="text-danger"></span>
                            
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-4">Groupe CM :</div>
                        <div class="col-10">
                            <input type="number" name="groupe_cm" id="groupe_cm" class="form-control">
                            
